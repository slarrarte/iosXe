<key>
	<chain xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-crypto">
		<name>{_key_chain_name}</name>
		<key>
			<id>{_id}</id>
			<key-string>
				<encryption>0</encryption>
				<key>{_key_string}</key>
			</key-string>
			<accept-lifetime>
				<lifetime-group-v1>
					<start-hh-mm-ss>{_a_start_hh_mm_ss}</start-hh-mm-ss>
					<start-month>{_a_start_month}</start-month>
					<start-day>{_a_start_day}</start-day>
					<start-year>{_a_start_year}</start-year>
					<end-hh-mm-ss>{_a_end_hh_mm_ss}</end-hh-mm-ss>
					<end-month>{_a_end_month}</end-month>
					<end-day>{_a_end_day}</end-day>
					<end-year>{_a_end_year}</end-year>
				</lifetime-group-v1>
			</accept-lifetime>
			<send-lifetime>
				<lifetime-group-v1>
					<start-hh-mm-ss>{_s_start_hh_mm_ss}</start-hh-mm-ss>
					<start-month>{_s_start_month}</start-month>
					<start-day>{_s_start_day}</start-day>
					<start-year>{_s_start_year}</start-year>
					<end-hh-mm-ss>{_s_end_hh_mm_ss}</end-hh-mm-ss>
					<end-month>{_s_end_month}</end-month>
					<end-day>{_s_end_day}</end-day>
					<end-year>{_s_end_year}</end-year>
				</lifetime-group-v1>
			</send-lifetime>
		</key>
	</chain>
</key>